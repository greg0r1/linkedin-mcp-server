{
  "name": "linkedin-mcp-server",
  "version": "1.0.0",
  "description": "LinkedIn MCP Server for Claude Desktop - Manage LinkedIn profile, company page, posts, and jobs",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
