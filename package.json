    "get-company-id": "node get-company-id.js",
    "test": "npm run build && npm start",
    "auth": "node dist/oauth-server.js"
  },
  "keywords": [
    "mcp",
    "linkedin",
    "claude",
    "ai",
    "automation"
  ],
